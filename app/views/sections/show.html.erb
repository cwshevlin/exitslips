<%= render 'layouts/teacher_section_header' %>
<%= render 'sections_sidebar' %>
<%= render 'quizzes_section_sidebar'%>

<div class="content">
<h1><%=@section.name%></h1>

<%= render 'section_data_table' %>

<%= link_to "Download as .csv", section_path(@section, format: "csv") %>
</div>

<%= hidden_field_tag :section_id, @section.id %>

<%= button_to 'Edit', {:controller => 'sections', :action => 'edit'}, {:method => :get} %>
<%= button_to 'Delete', section_path, :confirm => 'Are you sure?', :method => :delete %>

